{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "resolveJsonModule": true,
    "lib": ["es7"],
    "baseUrl": "./src",
    "paths": {
      "bell-interview-assignment": ["."]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
